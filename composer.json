{
    "name": "dmytrof/fractal-bundle",
    "type": "symfony-bundle",
    "description": "Symfony FractalBundle",
    "keywords": ["Fractal"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "league/fractal": "^0.18",
        "symfony/symfony": "^3.4|^4.3",
        "doctrine/common": "^2.7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofsymfony/rest-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\FractalBundle\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "suggest": {
        "league/fractal": "Allows more advanced using of Fractal",
        "friendsofsymfony/rest-bundle": "Adds support for using Fractal with FOSRestBundle"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x"
        }
    }
}